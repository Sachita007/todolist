<%- include("header") -%>
    
        <div class="box" id="heading">
            <h1>
                <%= listTitle %>
            </h1>
        </div>

        <div class="item-div box">
            

            <% for (let i=0; i<listitem.length; i++){ %>
                <div class="list-item"><input type="checkbox">
                    <p>
                        <%= listitem[i] %>
                    </p>
                </div>

                <% } %>

                   
                <form class="list-item" action="/" , method="post">
                    <input type="text" , id="data" name="data" placeholder="Add Item">
                    <button type="submit" name="list", value=<%= listTitle %>>+</button>
                </form>
                    
        </div>
    
   
        <%- include("footer") -%>